# Environment Variables for Samsung Memory Lens Backend

# Server Configuration
PORT=3000

# AWS Configuration (for image recognition)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1

# Qdrant Vector Database Configuration  
QDRANTDB_ENDPOINT=http://localhost:6333
QDRANTDB_API_KEY=your_qdrant_api_key_here

# Python Path (adjust for your system)
PYTHON_PATH=python

# Session Secret
SESSION_SECRET=your_session_secret_here